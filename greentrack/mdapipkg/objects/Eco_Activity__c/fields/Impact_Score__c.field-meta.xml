<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Impact_Score__c</fullName>
    <description>Stores the total impact score calculation.</description>
    <externalId>false</externalId>
    <formula>Trees_Planted__c * 21 + Waste_Recycled__c * 1.5</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Stores the total impact score calculation.</inlineHelpText>
    <label>Impact Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
